class maxArea{
    
    static int calculate_Area(Rectangle arr[]){
        
        // Your code here
        int maxi=arr[0].length*arr[0].breadth;
        for(int i=1;i<arr.length;i++)
        {
            int x=arr[i].length*arr[i].breadth;
            if(x>maxi)
                maxi=x;
        }
        return maxi;
    }
    
}
