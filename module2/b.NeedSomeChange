class Geeks{
    static void swapElements(int a[], int n){
        
        // Your code here
        //int temp[]=new int[n]
        for(int i=0;i<n-2;i++)
        {
            int t=a[i];
            a[i]=a[i+2];
            a[i+2]=t;
        }
        for(int i=0;i<n;i++)
            System.out.print(a[i]+" ");
        System.out.println();
        //if(n>0)
            //System.out.print(a[0]);  
        //for(int i=0;i<n;i++)
        
        
    }
}
